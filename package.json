{
  "name": "ke-taxi-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^1.3.0",
    "@capacitor/cli": "^1.2.1",
    "@capacitor/core": "^1.2.1",
    "@ionic/core": "^4.11.1",
    "@ionic/react": "^4.11.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.22",
    "@types/node": "12.7.12",
    "@types/react": "16.9.6",
    "@types/react-dom": "16.9.2",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^4.1.19",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "jest-enzyme": "^7.1.2",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.10.2",
    "react-google-maps": "^9.4.5",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "generate:andriod": "yarn build && npx cap add android",
    "sync:android": "yarn build && npx cap sync",
    "test": "CI=true react-scripts test --env=jsdom --transformIgnorePatterns \"node_modules/(?!(@ionic|ionicons))/\"",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json,css}'",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix 'src/**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,json,css,graphql,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "ts-jest": "^24.1.0"
  }
}
